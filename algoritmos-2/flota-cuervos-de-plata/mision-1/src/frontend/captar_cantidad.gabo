Procedimiento captar_cantidad(E/S: total: Entero);
Inicio
  var n, min, max: Entero;

  Mostrar << "Ingrese la cantidad total de tripulantes de toda la flota. ";
  Mostrar << "Solo numeros entre 2 y 500. ".;

  n = 0;
  min = 2;
  max = 500;
  captar_entero(n, min, max);
  Mostrar << captar_entero;

  total = n;
Fin_Procedimiento
